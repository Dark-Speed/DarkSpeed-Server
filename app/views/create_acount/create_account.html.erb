<html>
<head>
<h1>DarkSpeed Account Creation</h1>
</head>
<body>
  <%= form_tag '/create', multipart: true do %>
    Username:
    <%= text_field_tag :username %>
    <p></p>
    Password:
    <%= password_field_tag :password %>
    <p></p>
    Confirm Password:
    <%= password_field_tag :password_confirmation %>
    <p></p>
    Email:
    <%= text_field_tag :email %>
    <p></p>
    <%= submit_tag "Submit" %>
  <% end %>
</body>
</html>
