<html>
<head>

</head>
<body>
  <%= form_tag '/create', multipart: true do %>
    Username:
    <%= text_field_tag :username %>
    <p></p>
    Password:
    <%= text_field_tag :password %>
    <p></p>
    Confirm Password:
    <%= text_field_tag :confirm_password %>
    <p></p>
    Email:
    <%= text_field_tag :email %>
    <p></p>
    <%= submit_tag "Submit" %>
  <% end %>
</body>
</html>
